      <FunctionExtension
            class="com.siteview.workorder.odata.yft.GetOrderCount"
            descript="获取工单统计内容"
            functionName="GetOrderCount">
         <InputParameters>
            <Parameters
                  descript="创建人"
                  paramName="CREATEBY">
            </Parameters></InputParameters>
         <OutputParameters>
            <Parameters
                  descript="我参与的工单个数"
                  paramName="CY_COUNT">
            </Parameters>
            <Parameters
