                  paramName="CL_COUNT">
            </Parameters>
            <Parameters
                  descript="完成工单个数"
                  paramName="WC_COUNT">
            </Parameters>
            <Parameters
                  descript="所有工单个数"
                  paramName="ALL_COUNT">
            </Parameters>
            <Parameters
                  descript="未完成延期单"
                  paramName="INCOMPLETE_EX_COUNT">
            </Parameters>
            <Parameters
                  descript="已完成延期单"
                  paramName="COMPLETE_EX_COUNT">
            </Parameters>
